<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="JQuery Automation Testing" verbose="1">
    <parameter name="quickUrl" value="https://www.jqueryscript.net/demo/CRUD-Data-Grid-Plugin-jQuery-Quickgrid"/>
    <parameter name="appendUrl" value="https://www.jqueryscript.net/demo/jQuery-Dynamic-Data-Grid-Plugin-appendGrid"/>
    <parameter name="fileUploadUrl" value="https://blueimp.github.io/jQuery-File-Upload/"/>

    <test name="Run on Chrome browser">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="net.jqueryScript.Level_12_Upload_Files"/>
        </classes>
    </test>
</suite>